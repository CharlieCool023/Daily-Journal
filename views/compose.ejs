<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">

    <label for="composeText" class="form-label">Title</label>
    <input type="text" class="form-control" name="composeText" placeholder="">
    
    <label for="composePost" class="form-label">Post</label>
    <textarea class="form-control" name="composePost" rows="5"></textarea>
    
    <br>
    <button class="btn btn-primary" type="submit">Publish</button>
</form>



<%- include("partials/footer") -%>